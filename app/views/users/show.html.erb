
<% if current_user.member  %>
  <div id="profile" class="page-header header1">
    Gegevens van <%= @user.first_name %> <%= @user.last_name %>
  </div>
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="col-xs-12 col-md-6 home-box">
        <div class="home-inner-box">
          <div class="header1">Persoonlijke gegevens</div>
          <div class="text-box-l">

          </div>
        </div>
      </div>
      <div class="col-xs-12 col-md-6 home-box">
        <div class="home-inner-box">
          <div class="header1">Professionele gegevens</div>
          <div class="text-box-l">

          </div>
        </div>
      </div>
      <div class="col-xs-12 col-md-6 home-box">
        <div class="home-inner-box">
          <div class="header1">Praktijk gegevens</div>
          <div class="text-box-l">

          </div>
        </div>
      </div>
    </div>
  </div>
<% else %>
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="col-xs-12 col-md-6 home-box">
        <p>
          Uw aanvraag voor het lidmaatschap is in behandeling.
        </p>
        <%= link_to "terug", destroy_user_session_path, method: :delete, class: "knop knop-linksonder" %>
      </div>
    </div>
  </div>
<% end %>
